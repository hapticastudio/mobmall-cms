Listing Users

<table>
  <td>Email</td>
  <td>Admin</td>
  <td>Action</td>
  
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.admin? %></td>
      <% unless user == current_user %>
        <td><%= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Are you sure you want to remove #{user.email} account?"} %></td>
      <% end %>
    </tr>
  <% end %>
</table>
